{
  "console.info": {
    "prefix": "info",
    "body": [
      "console.info($1);$0"
    ]
  },
  "console.log": {
    "prefix": "log",
    "body": [
      "console.log($1);$0"
    ]
  },
  "console.warn": {
    "prefix": "warn",
    "body": [
      "console.warn($1);$0"
    ]
  },
  "console.error": {
    "prefix": "error",
    "body": [
      "console.error($1);$0"
    ]
  },
  "import": {
    "prefix": "import",
    "body": "import ${2:___} from \"${1:___}\";"
  },
  "arrow function (expression)": {
    "prefix": "ar",
    "body": "($1) => $2"
  },
  "arrow function (block)": {
    "prefix": "arr",
    "body": "($1) => {$2}"
  },
  "React function component": {
    "prefix": "fc",
    "body": [
      "function ${1:MyComponent}(${2:props}) {",
      "  return ${3:body};",
      "}$0"
    ]
  }
}
