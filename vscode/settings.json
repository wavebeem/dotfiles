{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnType": true,
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enabledFileTypes": {
    "*": false,
    "markdown": false
  },
  "cSpell.userWords": [
    "1upkeyboards",
    "arity",
    "Astro",
    "Astro's",
    "bytecode",
    "Cloudfront",
    "colocated",
    "colorjs",
    "Corne",
    "Ergodox",
    "esque",
    "fireteam",
    "gabapentin",
    "Gateron",
    "haha",
    "higlighting",
    "iframes",
    "Infobox",
    "infoboxes",
    "jacobolus",
    "Jamstack",
    "jsx-info",
    "Kailh",
    "KBDfans",
    "keeb",
    "Keeb",
    "Keebio",
    "keebs",
    "keymap",
    "Krytox",
    "Leatherman",
    "Lodash",
    "microcontroller",
    "netpet",
    "nonbinary",
    "Novelkeys",
    "numpad",
    "oklch",
    "ortholinear",
    "Parsimmon",
    "permalinks",
    "pkmn.help",
    "pluggable",
    "Polyoxymethylene",
    "prerendering",
    "Regexps",
    "runtimes",
    "Screenhive",
    "Tada68",
    "Taeha",
    "Tamagotchi",
    "toc",
    "wavebeem",
    "Webpack",
    "Zlant"
  ],
  "chat.commandCenter.enabled": false,
  "debug.console.fontSize": 14,
  "debug.terminal.clearBeforeReusing": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.renderSideBySide": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.dragAndDrop": false,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "JetBrains Mono, Menlo, Consolas, monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 16,
  "editor.fontWeight": 400,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 80,
  "editor.multiCursorModifier": "alt",
  "editor.overviewRulerBorder": false,
  "editor.padding.top": 40,
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "none",
  "editor.rulers": [80, 100, 120],
  "editor.scrollbar.horizontalScrollbarSize": 24,
  "editor.scrollbar.verticalScrollbarSize": 24,
  "editor.semanticHighlighting.enabled": false,
  "editor.stickyScroll.enabled": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.excludeLanguages": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.useFlatConfig": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.avsc": "json",
    "*.liquid": "html",
    "*.njk": "nunjucks",
    ".env": "ini",
    ".env.local.example": "ini",
    "CODEOWNERS": "ini",
    "bashrc": "shellscript",
    "gitignore": "ignore",
    "zshenv": "shellscript",
    "zshrc": "shellscript"
  },
  "files.autoSave": "onFocusChange",
  "files.defaultLanguage": "markdown",
  "files.exclude": {
    "**/__image_snapshots__/**/*": true,
    "**/_site": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.nyc_output": true,
    "**/.sass-cache": true,
    "**/.svn": true,
    "**/.venv": true,
    "**/*-min-*": true,
    "**/*-min.*": true,
    "**/*.egg-info": true,
    "**/*.log": true,
    "**/*.min.*": true,
    "**/*.min.css": true,
    "**/*.min.js": true,
    "**/*.pyc": true,
    "**/bundle.*": true,
    "**/coverage": true,
    "**/db.json": true,
    "**/log": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/storybook-static": true,
    "**/tmp": true
  },
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/build": true,
    "**/dist": true,
    "**/node_modules/**": true
  },
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.enabled": true,
  "git.fetchOnPull": true,
  "git.pruneOnFetch": true,
  "gitblame.statusBarMessageFormat": "${author.name} (${time.ago})",
  "html.format.indentHandlebars": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.fontFamily": "system-ui, sans-serif",
  "markdown.preview.fontSize": 18,
  "markdown.preview.lineHeight": 1.5,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "search.exclude": {
    "**/build": true,
    "**/dist": true
  },
  "search.smartCase": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.showExitAlert": false,
  "typescript.tsserver.log": "off",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.dialogStyle": "custom",
  "window.title": "${rootPath}${separator}${remoteName}${separator}Code",
  "workbench.activityBar.location": "top",
  "workbench.editor.centeredLayoutFixedWidth": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editorAssociations": {
    "*.csv": "default",
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": null,
  "workbench.preferredDarkColorTheme": "Miasma",
  "workbench.preferredHighContrastColorTheme": "Miasma",
  "workbench.preferredHighContrastLightColorTheme": "Miasma",
  "workbench.preferredLightColorTheme": "Miasma",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.indent": 24,
  "workbench.tree.renderIndentGuides": "always",
  "zenMode.fullScreen": false,
  "zenMode.showTabs": "single",
  "workbench.activityBar.location": "top",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.console.fontFamily": "",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.snippetSuggestions": "bottom",
  "workbench.editor.showTabs": "single",
  "window.menuBarVisibility": "compact",
  "workbench.colorTheme": "Fresh Green",
  "cSpell.enabled": false,
  "stylelint.validate": ["css", "postcss", "scss"],
  "window.menuStyle": "inherit",
  "explorer.confirmPasteNative": false,
  "window.titleSeparator": " â€¢ ",
  "editor.codeLensFontSize": 14,
  "editor.inlayHints.fontSize": 12,
  "scm.inputFontSize": 14,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  }
}
